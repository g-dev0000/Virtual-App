<?xml version="1.0" encoding="utf-8"?>
<manifest package="io.virtualapp"
          xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <application
        android:name=".VApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".enter.EnterActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".home.HomeActivity"/>
        <activity
            android:name=".home.ListAppActivity"
            android:theme="@style/AppListTheme"/>

        <activity android:name=".home.LoadingActivity" android:theme="@android:style/Theme.DeviceDefault.Dialog.NoActionBar" android:noHistory="true" android:taskAffinity="va.task.loading"/>


        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5792f68f67e58e08ee000ee1">
        </meta-data>
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="Umeng">
        </meta-data>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>


</manifest>